{
  "compilerOptions": {
    "target": "esnext",
    "lib": ["esnext", "dom"],
    "strict": true,
    "jsx": "react",
    "jsxFactory": "Didact.createElement",
    "moduleResolution": "node",
    "rootDir": "./MyAddon",
    "typeRoots": ["./@types", "./node_modules/@types"],
    "noUnusedLocals": true,
    "noUnusedParameters": true
  },
  "tstl": {
    "luaTarget": "5.1",
    "luaLibImport": "none"
  }
}
