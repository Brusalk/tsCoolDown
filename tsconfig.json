{
  "compilerOptions": {
    "target": "esnext",
    "lib": ["esnext", "dom"],
    "strict": true,
    "jsx": "react",
    "jsxFactory": "Didact.createElement",
    "moduleResolution": "node",
    "rootDir": "tsCoolDown",
    "outDir": "./dist",
    "typeRoots": [
      "node_modules/@types",
      "node_modules/@wartoshika/wow-declarations"
    ]
  },
  "exclude": ["build/"],
  "tstl": {
    "luaTarget": "5.1",
    "luaLibImport": "none"
  }
}
